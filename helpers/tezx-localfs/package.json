{
    "name": "@tezx/localfs",
    "version": "1.0.4",
    "type": "module",
    "description": "Local file system helper for TezX framework. Easily upload, read, and delete files on local storage.",
    "keywords": [
        "tezx",
        "localfs",
        "filesystem",
        "upload",
        "file-storage",
        "nodejs",
        "bun",
        "deno"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.mjs"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tezxjs/tezx-toolkit"
    },
    "bugs": {
        "url": "https://github.com/tezxjs/tezx-toolkit/issues"
    },
    "homepage": "https://github.com/tezxjs/tezx-toolkit/blob/main/helpers/tezx-localfs/README.md",
    "engines": {
        "node": ">=18",
        "bun": ">=1.0"
    },
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    }
}