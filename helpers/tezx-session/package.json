{
    "name": "@tezx/session",
    "version": "1.0.1",
    "type": "module",
    "description": "A simple and extensible session middleware for TezX framework with pluggable storage and secure cookie support.",
    "keywords": [
        "tezx",
        "session",
        "middleware",
        "typescript",
        "cookie",
        "storage",
        "redis"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.mjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/tezxjs/tezx-toolkit"
    },
    "bugs": {
        "url": "https://github.com/tezxjs/tezx-toolkit/issues"
    },
    "homepage": "https://github.com/tezxjs/tezx-toolkit/blob/main/helpers/tezx-session/README.md",
    "engines": {
        "node": ">=18",
        "bun": ">=1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}