{
  "name": "tezx",
  "version": "1.0.0",
  "description": "TezX is a high-performance, lightweight JavaScript framework designed for speed, scalability, and flexibility. It enables efficient routing, middleware management, and static file serving with minimal configuration. Fully compatible with Bun.",
  "scripts": {
    "start": "bun dist/index.js",
    "pk": "pkgroll --clean-dist --output-exports named",
    "dev": "bun run --hot --watch index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tezxjs/tezx-app-example"
  },
  "keywords": [],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tezxjs/tezx-app-example"
  },
  "homepage": "https://github.com/tezxjs/tezx-app-example",
  "dependencies": {
    "@types/bun": "^1.3.3",
    "pkgroll": "^2.13.1",
    "tezx": "^4.0.7",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14"
  }
}