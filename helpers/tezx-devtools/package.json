{
  "name": "tezx-devtools",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist",
    "build:cjs": "tsc --module CommonJS --outDir package/cjs --removeComments",
    "build:esm": "tsc --module ESNext --outDir package --removeComments",
    "build:dts": "tsc --module ESNext --outDir package --declaration --emitDeclarationOnly",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:dts",
    "bun": "bun run --hot --watch app.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srakib17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/srakib17"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/srakib17",
  "dependencies": {
    "tezx": "^4.0.9"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3"
  }
}
